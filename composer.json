{
    "name": "arielenter/laravel-test-translations",
    "version": "1.0.2",
    "dist": {
        "url": "https://github.com/arielenter/laravel-test-translations/archive/refs/tags/v1.0.2.tar.gz",
        "type": "tar"
    },
    "description": "Easy way to test the existence of translations, placeholders and replace keys.",
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Arielenter\\LaravelTestTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ariel Del Valle Lozano",
            "email": "arielmazatlan@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^10.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arielenter\\LaravelTestTranslations\\ServiceProvider"
            ]
        }
    }
}
